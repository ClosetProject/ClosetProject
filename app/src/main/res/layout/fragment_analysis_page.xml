<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.analysisPage"
    android:id="@+id/frameLayout4" >

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="Personal Color"
                    android:textColor="#000000"
                    android:textSize="22sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="155dp">

                <LinearLayout
                    android:layout_width="110dp"
                    android:layout_height="140dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/piple" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:orientation="horizontal">

                <com.github.mikephil.charting.charts.HorizontalBarChart
                    android:id="@+id/bar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="5dp"
                    android:padding="0dp">

                </com.github.mikephil.charting.charts.HorizontalBarChart>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="당신의 퍼스널 컬러는 봄웜 입니다"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/finish_lin4"
                        android:layout_width="410dp"
                        android:layout_height="11dp"
                        android:layout_weight="1"
                        android:background="@drawable/figure"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView39"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="clip_horizontal"
                        android:paddingLeft="25dp"
                        android:paddingTop="10dp"
                        android:text="닉네임님에게 어울리는 색상"
                        android:textColor="#000000"
                        android:textSize="15dp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="79dp"
                    android:orientation="horizontal">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textView40"
                            android:layout_width="113dp"
                            android:layout_height="35dp"
                            android:layout_marginStart="25dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="BEST color"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <View
                            android:id="@+id/line8"
                            android:layout_width="150dp"
                            android:layout_height="2dp"
                            android:layout_gravity="start"
                            android:layout_marginStart="20dp"
                            android:layout_weight="1"
                            android:background="#BDBDBD"
                            app:layout_constraintEnd_toEndOf="@+id/textView40"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView40"
                            tools:ignore="MissingConstraints" />

                        <TextView
                            android:id="@+id/goodcolor"
                            android:layout_width="113dp"
                            android:layout_height="35dp"
                            android:layout_marginStart="80dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="GOOD color"
                            android:textColor="#000000"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@+id/textView40"
                            app:layout_constraintStart_toEndOf="@+id/textView40"
                            app:layout_constraintTop_toTopOf="@+id/textView40" />

                        <View
                            android:id="@+id/line10"
                            android:layout_width="150dp"
                            android:layout_height="2dp"
                            android:layout_gravity="start"
                            android:layout_weight="1"
                            android:background="#BDBDBD"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/goodcolor"
                            app:layout_constraintHorizontal_bias="0.263"
                            app:layout_constraintStart_toStartOf="@+id/goodcolor"
                            app:layout_constraintTop_toBottomOf="@+id/goodcolor"
                            app:layout_constraintVertical_bias="0.0"
                            tools:ignore="MissingConstraints" />

                        <ImageView
                            android:id="@+id/img_best"
                            android:layout_width="145dp"
                            android:layout_height="30dp"
                            android:layout_marginStart="20dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView40"
                            tools:src="@drawable/sw_best" />

                        <ImageView
                            android:id="@+id/img_good"
                            android:layout_width="145dp"
                            android:layout_height="30dp"
                            android:layout_marginStart="35dp"
                            app:layout_constraintBottom_toBottomOf="@+id/img_best"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.145"
                            app:layout_constraintStart_toEndOf="@+id/img_best"
                            app:layout_constraintTop_toBottomOf="@+id/line8"
                            app:layout_constraintVertical_bias="1.0"
                            tools:src="@drawable/sw_good" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView42"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:paddingLeft="20dp"
                        android:text="FASHION color comblnatlons"
                        android:textColor="#000000"
                        android:textStyle="bold" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="361dp">>
                    <Button
                        android:id="@+id/btn_top"
                        android:layout_width="82dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="36dp"
                        android:backgroundTint="#FFFFFF"
                        android:text="상의"
                        android:textColor="#000000"
                        android:textSize="12dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TouchTargetSizeCheck" />

                    <Button
                        android:id="@+id/btn_b"
                        android:layout_width="82dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="44dp"
                        android:backgroundTint="#FFFFFF"
                        android:text="하의"
                        android:textColor="#000000"
                        android:textSize="12dp"
                        app:layout_constraintStart_toEndOf="@+id/btn_top"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TouchTargetSizeCheck" />

                    <Button
                        android:id="@+id/btn_d"
                        android:layout_width="82dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="44dp"
                        android:backgroundTint="#FFFFFF"
                        android:text="드레스"
                        android:textColor="#000000"
                        android:textSize="12dp"
                        app:layout_constraintStart_toEndOf="@+id/btn_b"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="TouchTargetSizeCheck" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btn_b">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingStart="40dp"
                                android:paddingTop="10dp">

                                <ImageView
                                    android:id="@+id/best_img1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:src="@drawable/p_pani" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingTop="10dp"
                                android:paddingEnd="40dp">

                                <ImageView
                                    android:id="@+id/best_img2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:src="@drawable/p_lrod" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingStart="40dp"
                                android:paddingTop="10dp">

                                <ImageView
                                    android:id="@+id/best_img3"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:paddingTop="10dp"
                                    android:src="@drawable/p_tano" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:paddingTop="10dp"
                                android:paddingEnd="40dp">

                                <ImageView
                                    android:id="@+id/best_img4"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:src="@drawable/p_mall" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>